{% extends "AppBundle::layout.html.twig" %}

{% block subheader %}
	{% include 'AppBundle:Commons:kpiNav.html.twig' %}
{% endblock %}

{% block content %}

	{% block clientsFilter %}
		{% include 'AppBundle:Commons:kpiFilter.html.twig' %}
	{% endblock %}


	<div class="text-center col-xs-12">
		<p>Les résultats ci-dessous sont mis à jour toutes les semaines</p>
	</div>

	<div id="ajax-content">

	{% if currentKpi.nbTransacS0 > 0 %}

	<div class="col-xs-12">
		<h2 class="text-uppercase gris-bg">{{ "La qualification de mes clients"|trans }}</h2>

		<div class="col-xs-12 kpi-box">

			{% if user_role != "ROLE_VENDEUR" and currentKpi.txTransacLinkedS0 > 0 %}
				<div class="inside col-xs-12 bordered-bottom">
					<h3 class="text-uppercase text-center gris">Identification des clients</h3>
						<p class="col-xs-12 text-center">
							<strong>{{ currentKpi.nbTransacS0|number_format(0, ',', ' ') }} Transactions enregistrées</strong>
						</p>
					<figure class="col-xs-12">
						<div id="chartContainer0" class="col-xs-12" style="height: 250px;"></div>
					<figcaption class="col-xs-12 col-md-4">
						<p class="small">
							&nbsp;<br />
							<i style="color: #53B3AD;" class="fa fa-square fa-lg" aria-hidden="true"></i><br />
							{{ "Transactions identifiées avec un nom & un prénom et avec au moins un moyen de contact valide et optin"|trans}}
						</p>
					</figcaption>

					<figcaption class="col-xs-12 col-md-4">
						<p class="small">
							&nbsp;<br />
							<i style="color: #F5F4F2;" class="fa fa-square fa-lg" aria-hidden="true"></i><br />
							{{ "Transactions identifiées avec un nom & un prénom sans moyen de contact"|trans}}
						</p>
					</figcaption>

					<figcaption class="col-xs-12 col-md-4">
						<p class="small">
							&nbsp;<br />
							<i style="color: #393845;" class="fa fa-square fa-lg" aria-hidden="true"></i><br />
							{{ "Transactions non liées (sans nom & prénom)"|trans}}
						</p>
					</figcaption>
					</figure>
				</div>
			{% endif %}

			<div class="inside col-xs-12 bordered-bottom">
				<h4 class="text-uppercase black text-center">Capture par canal{% if  currentKpi.txTransacLinkedS0 == 0 %}<br />{{ currentKpi.nbTransacS0|number_format(0, ',', ' ') }} Transactions enregistrées{% endif %}</h4>

				<figure class="col-xs-12 col-md-3">
					<h4 class="text-center bg-white black">Email<br />&nbsp;</h4>
					<div id="chartContainer1" style="height: 150px;"></div>
				</figure>
				<figure class="col-xs-12 col-md-3">
					<h4 class="text-center bg-white black">SMS<br />&nbsp;</h4>
					<div id="chartContainer2" style="height: 150px;"></div>
				</figure>
				<figure class="col-xs-12 col-md-3">
					<h4 class="text-center bg-white black">Adresse<br />&nbsp;</h4>
					<div id="chartContainer3" style="height: 150px;"></div>
				</figure>
				<figure class="col-xs-12 col-md-3">
					<h4 class="text-center bg-white black">Email ou SMS<br />&nbsp;</h4>
					<div id="chartContainer4" style="height: 150px;"></div>
				</figure>
				<figure class="col-xs-12 col-md-3">
					<figcaption>
						<p class="small">
							<i style="color: #93A603;" class="fa fa-square fa-lg" aria-hidden="true"></i>
							<i style="color: #05AFF2;" class="fa fa-square fa-lg" aria-hidden="true"></i>
							<i style="color: #F0948A;" class="fa fa-square fa-lg" aria-hidden="true"></i>
							<i style="color: #FFA118;" class="fa fa-square fa-lg" aria-hidden="true"></i><br />
							{{ "Taux de transactions avec des données valides et optin"|trans}}
						</p>
					</figcaption>
				</figure>
				<figure class="col-xs-12 col-md-3">
					<figcaption>
						<p class="small">
							<i style="color: #A9BF04;" class="fa fa-square fa-lg" aria-hidden="true"></i>
							<i style="color: #05C7F2;" class="fa fa-square fa-lg" aria-hidden="true"></i>
							<i style="color: #FFD9D8;" class="fa fa-square fa-lg" aria-hidden="true"></i>
							<i style="color: #FFD14A;" class="fa fa-square fa-lg" aria-hidden="true"></i><br />
							{{ "Taux de transactions avec des données valides et optout"|trans}}
						</p>
					</figcaption>
				</figure>
				<figure class="col-xs-12 col-md-3">
					<figcaption>
						<p class="small">
							<i style="color: #F5F4F2;" class="fa fa-square fa-lg" aria-hidden="true"></i><br />
							{{ "Taux de transactions avec des données non-collectées"|trans}}
						</p>
					</figcaption>
				</figure>
				<figure class="col-xs-12 col-md-3">
					<figcaption>
						<p class="small">
							<i style="color: #393845;" class="fa fa-square fa-lg" aria-hidden="true"></i><br />
							{{ "Taux de transactions avec des données non-valides"|trans}}
						</p>
					</figcaption>
				</figure>
			</div>


			<div class="inside col-xs-12">
				<h4 class="text-uppercase black text-center">{{ "Evolution mensuelle des indicateurs de collecte"|trans }}</h4>

				<figure class="col-xs-12 col-md-8 col-md-offset-2">
					<div id="chartContainer5" class="col-xs-12" style="height: 194px;"></div>
				</figure>

				<figure class="col-xs-12">
					<figcaption class="col-xs-12 col-md-3">
						<p class="small">
							&nbsp;<br />
							<i style="color: #93A603;" class="fa fa-square fa-lg" aria-hidden="true"></i><br />
							{{ "Taux de transactions avec des emails valides et optin"|trans}}
						</p>
					</figcaption>
					<figcaption class="col-xs-12 col-md-3">
						<p class="small">
							&nbsp;<br />
							<i style="color: #05AFF2;" class="fa fa-square fa-lg" aria-hidden="true"></i><br />
							{{ "Taux de transactions avec des SMS valides et optin"|trans}}
						</p>
					</figcaption>
					<figcaption class="col-xs-12 col-md-3">
						<p class="small">
							&nbsp;<br />
							<i style="color: #F0948A;" class="fa fa-square fa-lg" aria-hidden="true"></i><br />
							{{ "Taux de transactions avec des adresses valides et optin"|trans}}
						</p>
					</figcaption>
					<figcaption class="col-xs-12 col-md-3">
						<p class="small">
							&nbsp;<br />
							<i style="color: #FFA118;" class="fa fa-square fa-lg" aria-hidden="true"></i><br />
							{{ "Taux de transactions avec des emails ou SMS valides et optin"|trans}}
						</p>
					</figcaption>
					<figcaption class="col-xs-12 col-md-3">
						<p class="small">
							&nbsp;<br />
							<i style="color: #53B3AD;" class="fa fa-square fa-lg" aria-hidden="true"></i><br />
							{{ "Taux de transactions avec des emails ou SMS ou adresses valides et optin"|trans}}
						</p>
					</figcaption>
				</figure>
				<div class="spacer"></div>
				<div class="col-xs-12 text-center">
					<a target="_blank" href="{{ path('app_kpi_faq', { 'user_actuel' : userId, 'user_id' : user.id } ) }}" class="small" style="color:#666; text-decoration:underline;">
			    		{{"Définitions et règles de calcul dans l'onglet FAQ"|trans}}
			    	</a>
				</div>
			</div>
		</div>

	</div>

	{% else %}
		<div class="col-xs-12">
			<h2 class="text-uppercase gris-bg text-center">Aucune transactions pour la date demandée</h2>
		</div>
	{% endif %}

	<div class="spacer" style="margin-bottom:0px;"></div>

	{% if user_role != "ROLE_VENDEUR" %}
		<div class="col-xs-12">
			{% if user_role == "ROLE_MARQUE" %}
				<h2 class="text-uppercase gris-bg">Performances des DR du réseau {{user.username}}</h2>
			{% elseif user_role == "ROLE_DR" %}
				<h2 class="text-uppercase gris-bg">Performances des Boutiques du DR {{user.username}}</h2>
			{% elseif user_role == "ROLE_BOUTIQUE" %}
				<h2 class="text-uppercase gris-bg">Performances des Vendeurs de la Boutique {{user.username}}</h2>
			{% endif %}
			<div class="col-xs-12 kpi-box">
				<table class="table table-bordered table-hover" style="margin-bottom:0;">
					<thead>
						<tr>
							<th class=""></th>
							<th class=" text-center text-uppercase">Nombre de transactions</th>
							<th class=" text-center text-uppercase">Taux de transactions liées</th>
							<th class=" text-center text-uppercase">Taux de transactions liées valides et optin</th>
							<th class=" text-center text-uppercase">Capture emails valides et optin</th>
							<th class=" text-center text-uppercase">Capture SMS valides et optin</th>
							<th class=" text-center text-uppercase">Capture adresses valides et optin</th>
							<th class=" text-center text-uppercase">Capture emails ou SMS valide et optin</th>
						</tr>
					</thead>
					<tbody>
						<tr class="info">
							<th class="text-right text-uppercase" style="color: #3399FF">Résultats du réseau</th>
							<td class="text-center">{{marque.nbTransacS0|number_format(0, ',', ' ') }}</td>
							<td class="text-center">{{ marque.txTransacLinkedS0|number_format}} %</td>
							<td class="text-center">{{ marque.txTransacLinkedOptinS0|number_format}} %</td>
							<td class="text-center">{{ marque.txTransacNpeiS0|number_format}} %</td>
							<td class="text-center">{{ marque.txTransacNpsiS0|number_format}} %</td>
							<td class="text-center">{{ marque.txTransacNpaiS0|number_format}} %</td>
							<td class="text-center">{{ marque.txTransacNpesi2S0|number_format}} %</td>
						</tr>
						{% if user_role == "ROLE_BOUTIQUE" or user_role == "ROLE_DR" %}
							<tr class="info">
								<th class="text-right text-uppercase" style="color: #3399FF">{{currentKpi.user.username}}</th>
								<td class="text-center">{{ currentKpi.nbTransacS0|number_format(0, ',', ' ') }}</td>
								<td class="text-center">{{ currentKpi.txTransacLinkedS0|number_format}} %</td>
								<td class="text-center">{{ currentKpi.txTransacLinkedOptinS0|number_format}} %</td>
								<td class="text-center">{{ currentKpi.txTransacNpeiS0|number_format}} %</td>
								<td class="text-center">{{ currentKpi.txTransacNpsiS0|number_format}} %</td>
								<td class="text-center">{{ currentKpi.txTransacNpaiS0|number_format}} %</td>
								<td class="text-center">{{ currentKpi.txTransacNpesi2S0|number_format}} %</td>
							</tr>
						{% endif %}
						{% if user_role == "ROLE_MARQUE" %}
							{% for boutique in getDrsMarque %}
								{% set keyloop = loop.index %}
								{% if getDrsMarque is not null %}
									<tr>
										<th class="text-right text-uppercase"><i class="fa fa-plus-circle" aria-hidden="true" data="boutiques_{{keyloop}}" style="cursor:pointer;"></i>&nbsp;&nbsp;<a target="_blank" href="{{ path('app_kpi_month', { 'user_actuel' : userId, 'user_id' : boutique.user.id } ) }}">{{boutique.user.username}}</a></th>
										<td class="text-center">{{ boutique.nbTransacS0|number_format(0, ',', ' ') }}</td>
										<td class="text-center">{{ boutique.txTransacLinkedS0|number_format}} %</td>
										<td class="text-center">{{ boutique.txTransacLinkedOptinS0|number_format}} %</td>
										<td class="text-center">{{ boutique.txTransacNpeiS0|number_format}} %</td>
										<td class="text-center">{{ boutique.txTransacNpsiS0|number_format}} %</td>
										<td class="text-center">{{ boutique.txTransacNpaiS0|number_format}} %</td>
										<td class="text-center">{{ boutique.txTransacNpesi2S0|number_format}} %</td>
									</tr>
								{% endif %}
								{% for boutique2 in getBoutiquesDr %}
									<tbody class="boutiques_{{keyloop}}" style="display:none;">
									{% for boutique3 in boutique2 %}
										{% if boutique3.user.dr == boutique.user.username %}
											<tr>
												<th class="text-right text-uppercase small"><a target="_blank" href="{{ path('app_kpi_month', { 'user_actuel' : userId, 'user_id' : boutique3.user.id } ) }}">{{boutique3.user.username}}</a></th>
												<td class="text-center">{{ boutique3.nbTransacS0|number_format(0, ',', ' ') }}</td>
												<td class="text-center">{{ boutique3.txTransacLinkedS0|number_format}} %</td>
												<td class="text-center">{{ boutique3.txTransacLinkedOptinS0|number_format}} %</td>
												<td class="text-center">{{ boutique3.txTransacNpeiS0|number_format}} %</td>
												<td class="text-center">{{ boutique3.txTransacNpsiS0|number_format}} %</td>
												<td class="text-center">{{ boutique3.txTransacNpaiS0|number_format}} %</td>
												<td class="text-center">{{ boutique3.txTransacNpesi2S0|number_format}} %</td>
											</tr>
										{% endif %}
									{% endfor %}
									</tbody>
								{% endfor %}
							{% endfor %}
						{% elseif user_role == "ROLE_DR" %}
							{% for boutique in getBoutiquesDr %}
								{% set keyloop = loop.index %}
								{% if getBoutiquesDr is not null and boutique.nbTransacS0 > 0 %}
									<tr>
										<th class="text-right text-uppercase"><i class="fa fa-plus-circle" aria-hidden="true" data="boutiques_{{keyloop}}" style="cursor:pointer;"></i>&nbsp;&nbsp;<a target="_blank" href="{{ path('app_kpi_month', { 'user_actuel' : userId, 'user_id' : boutique.user.id } ) }}">{{boutique.user.username}}</a></th>
										<td class="text-center">{{ boutique.nbTransacS0|number_format(0, ',', ' ') }}</td>
										<td class="text-center">{{ boutique.txTransacLinkedS0|number_format}} %</td>
										<td class="text-center">{{ boutique.txTransacLinkedOptinS0|number_format}} %</td>
										<td class="text-center">{{ boutique.txTransacNpeiS0|number_format}} %</td>
										<td class="text-center">{{ boutique.txTransacNpsiS0|number_format}} %</td>
										<td class="text-center">{{ boutique.txTransacNpaiS0|number_format}} %</td>
										<td class="text-center">{{ boutique.txTransacNpesi2S0|number_format}} %</td>
									</tr>
								{% endif %}
								{% for boutique2 in getVendeursBoutique %}
									<tbody class="boutiques_{{keyloop}}" style="display:none;">
									{% for boutique3 in boutique2 %}
										{% if boutique3.user.boutique == boutique.user.username %}
											<tr>
												<th class="text-right text-uppercase small"><a target="_blank" href="{{ path('app_kpi_month', { 'user_actuel' : userId, 'user_id' : boutique3.user.id } ) }}">{{boutique3.user.nameAndSurname}}</a></th>
												<td class="text-center">{{ boutique3.nbTransacS0|number_format(0, ',', ' ') }}</td>
												<td class="text-center">{{ boutique3.txTransacLinkedS0|number_format}} %</td>
												<td class="text-center">{{ boutique3.txTransacLinkedOptinS0|number_format}} %</td>
												<td class="text-center">{{ boutique3.txTransacNpeiS0|number_format}} %</td>
												<td class="text-center">{{ boutique3.txTransacNpsiS0|number_format}} %</td>
												<td class="text-center">{{ boutique3.txTransacNpaiS0|number_format}} %</td>
												<td class="text-center">{{ boutique3.txTransacNpesi2S0|number_format}} %</td>
											</tr>
										{% endif %}
									{% endfor %}
									</tbody>
								{% endfor %}
							{% endfor %}
						{% elseif user_role == "ROLE_BOUTIQUE" %}
							{% for boutique in getVendeursBoutique %}
								{% if getVendeursBoutique is not null and boutique.nbTransacS0 > 0 %}
									<tr>
										<th class="text-right text-uppercase"><a target="_blank" href="{{ path('app_kpi_month', { 'user_actuel' : userId, 'user_id' : boutique.user.id } ) }}">{{boutique.user.nameAndSurname}}</a></th>
										<td class="text-center">{{ boutique.nbTransacS0|number_format(0, ',', ' ') }}</td>
										<td class="text-center">{{ boutique.txTransacLinkedS0|number_format}} %</td>
										<td class="text-center">{{ boutique.txTransacLinkedOptinS0|number_format}} %</td>
										<td class="text-center">{{ boutique.txTransacNpeiS0|number_format}} %</td>
										<td class="text-center">{{ boutique.txTransacNpsiS0|number_format}} %</td>
										<td class="text-center">{{ boutique.txTransacNpaiS0|number_format}} %</td>
										<td class="text-center">{{ boutique.txTransacNpesi2S0|number_format}} %</td>
									</tr>
								{% endif %}
							{% endfor %}
						{% endif %}
					</tbody>
				</table>
			</div>
		</div>
	{% endif %}



	<div class="spacer" style="margin-bottom:0px;"></div>

	<div class="col-xs-12">

		{% if user_role != "ROLE_VENDEUR" %}
			<h2 class="text-uppercase gris-bg">Le top boutique {{ user.brand }}</h2>
			<div class="col-xs-12 kpi-box">
				<div class="inside">
					<div class="inside top col-xs-4 text-center">
						<h3 class="black">#1 Email</h3>
						<div class="dark-box">
							<h3>{{ deleteFirstCharacters(topNpe.user.username,4)|upper }}</h3>
							<p class="small white">{{ topNpe.txTransacNpeS0|number_format(0, ',', ' ') }} % {{ "de transactions"|trans }}<br /> {{ "Emails valides"|trans }}</p>
						</div>
					</div>
					<div class="inside top col-xs-4 text-center">
						<h3 class="black">#1 Email + SMS</h3>
						<div class="dark-box">
							<h3>{{ deleteFirstCharacters(topNpes.user.username, 4)|upper }}</h3>
							<p class="small white">{{ topNpes.txTransacNpesS0|number_format(0, ',', ' ') }} % {{ "de transactions"|trans }}<br /> {{ "Emails + SMS valides"|trans }}</p>
						</div>
					</div>
					<div class="inside top col-xs-4 text-center">
						<h3 class="black">#1 Email + SMS + Adresse</h3>
						<div class="dark-box">
							<h3>{{ deleteFirstCharacters(topNpesa.user.username,4)|upper }}</h3>
							<p class="small white">{{ topNpesa.txTransacNpesaS0|number_format(0, ',', ' ') }} % {{ "de transactions"|trans }}<br /> {{ "Emails + SMS + Adresses valides"|trans }}</p>
						</div>
					</div>
					<div style="height:0px;" class="spacer"></div>
				</div>
			</div>
		{% else %}
			<h2 class="text-uppercase gris-bg">Le top Vendeur {{ user.brand }}</h2>
			<div class="col-xs-12 kpi-box">
				<div class="inside">
					<div class="inside top col-xs-4 text-center">
						<h3 class="black">#1 Email</h3>
						<div class="dark-box">
							<h3>{{ topNpeVendeur.user.nameAndSurname|upper }}</h3>
							<p class="small white">{{ topNpeVendeur.txTransacNpeS0|number_format(0, ',', ' ') }} % {{ "de transactions"|trans }}<br /> {{ "Emails valides"|trans }}</p>
						</div>
					</div>
					<div class="inside top col-xs-4 text-center">
						<h3 class="black">#1 Email + SMS</h3>
						<div class="dark-box">
							<h3>{{ topNpesVendeur.user.nameAndSurname|upper }}</h3>
							<p class="small white">{{ topNpesVendeur.txTransacNpesS0|number_format(0, ',', ' ') }} % {{ "de transactions"|trans }}<br /> {{ "Emails + SMS valides"|trans }}</p>
						</div>
					</div>
					<div class="inside top col-xs-4 text-center">
						<h3 class="black">#1 Email + SMS + Adresse</h3>
						<div class="dark-box">
							<h3>{{ topNpesaVendeur.user.nameAndSurname|upper }}</h3>
							<p class="small white">{{ topNpesaVendeur.txTransacNpesaS0|number_format(0, ',', ' ') }} % {{ "de transactions"|trans }}<br /> {{ "Emails + SMS + Adresses valides"|trans }}</p>
						</div>
					</div>
					<div style="height:0px;" class="spacer"></div>
				</div>
			</div>
		{% endif %}
	</div>
	</div>

	<div style="height:30px;" class="spacer"></div>

{% endblock %}

{% block canvasJs %}

	<script type="text/javascript">

	$(function(){
    	$(document).on('change', '#appbundle_kpi_filter_week', function() {
    		$('#clients-filter').submit();
    		$('#spinner').show();
    	});


    	$(document).on('click', '.fa-plus-circle', function() {
    		$(this).removeClass('fa-plus-circle');
    		$(this).addClass('fa-minus-circle');

    		var id = $(this).attr('data');
    		//$('.'+id).removeClass('hidden');
    		$('.'+id).show();
    	});


    	$(document).on('click', '.fa-minus-circle', function() {
    		$(this).removeClass('fa-minus-circle');
    		$(this).addClass('fa-plus-circle');

    		var id = $(this).attr('data');
    		//$('.'+id).addClass('hidden');
    		$('.'+id).hide();
    	});

    	$(document).on('change', '#appbundle_kpi_filter_reseau', function() {
    		// Ici on réinitialise la page au niveau Marque au changement
    		//pour éviter le bug d'envoie de formulaire vide si on sélectionne autre chose après

    		$('#appbundle_kpi_filter_submit').attr('disabled','disabled');
    		$('#appbundle_kpi_filter_boutique').val(null).prop('disabled',true);
    		$('#appbundle_kpi_filter_dr').val(null).prop('disabled',true);
    		$('#appbundle_kpi_filter_vendeur').val(null).prop('disabled',true);

    		$('#clients-filter').submit();
    		$('#spinner').show();
    	});
    	$(document).on('change', '#appbundle_kpi_filter_dr', function() {
    		$('#appbundle_kpi_filter_submit').attr('disabled','disabled');
    		$('#appbundle_kpi_filter_boutique').val(null).prop('disabled',true);
    		$('#appbundle_kpi_filter_vendeur').val(null).prop('disabled',true);

    		/*if( $(this).val() == "" && $('#appbundle_kpi_filter_boutique').val() == "" ){
    			var user_id = $('#appbundle_kpi_filter_reseau').val();
    		}
    		else if( $(this).val() == "" && $('#appbundle_kpi_filter_boutique').val() != "" ){
    			var user_id = $('#appbundle_kpi_filter_boutique').val();
    		}
    		else{
    			var user_id = $(this).val();
